{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksConfig": {
      "phases": {
        "setup": {
          "nixPkgs": ["python313"]
        },
        "install": {
          "cmds": [
            "python -m pip install --upgrade pip",
            "pip install -r requirements.txt"
          ]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "python userbot.py",
    "restartPolicyType": "ON_FAILURE"
  }
}
